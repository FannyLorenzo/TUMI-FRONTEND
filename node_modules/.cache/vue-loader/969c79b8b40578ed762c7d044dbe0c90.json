gmail.com\"\n        filled\n        background-color=\"transparent\"\n      ></v-text-field>\n      <v-text-field\n        v-model=\"password\"\n        filled\n        background-color=\"transparent\"\n        :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\n        :rules=\"[rules.required, rules.min]\"\n        :type=\"show1 ? 'text' : 'password'\"\n        name=\"input-10-1\"\n        label=\"Password\"\n        hint=\"At least 8 characters\"\n        counter\n        @click:append=\"show1 = !show1\"\n      ></v-text-field>\n      <v-text-field\n        v-model=\"placetext\"\n        label=\"Placeholder\"\n        placeholder=\"Enter Placeholder\"\n        filled\n        background-color=\"transparent\"\n      ></v-text-field>\n      <v-textarea\n        filled\n        name=\"input-7-4\"\n        rows=\"3\"\n        label=\"Textarea\"\n        value\n        background-color=\"transparent\"\n      ></v-textarea>\n      <v-text-field\n        v-model=\"disableinput\"\n        label=\"Disabled Input\"\n        disabled\n        filled\n        background-color=\"transparent\"\n      ></v-text-field>\n      <v-row>\n        <v-col cols=\"12\" lg=\"6\">\n          <v-checkbox\n            v-model=\"checkbox1\"\n            class=\"mt-2\"\n            label=\"Check this custom checkbox\"\n            hide-details\n          ></v-checkbox>\n          <v-checkbox\n            v-model=\"checkbox2\"\n            class=\"mt-2\"\n            label=\"Check this custom checkbox\"\n            hide-details\n          ></v-checkbox>\n          <v-checkbox\n            v-model=\"checkbox3\"\n            class=\"mt-2\"\n            label=\"Check this custom checkbox\"\n            hide-details\n          ></v-checkbox>\n          <div class=\"mt-8\">\n            <v-switch v-model=\"switch1\" inset label=\"Toggle this switch element\" hide-details></v-switch>\n            <v-switch v-model=\"switch2\" disabled inset label=\"Disabled switch element\" hide-details></v-switch>\n          </div>\n        </v-col>\n        <v-col cols=\"12\" lg=\"6\">\n          <v-radio-group v-model=\"radios\" :mandatory=\"false\">\n            <v-radio label=\"Toggle this custom radio\" value=\"radio1\"></v-radio>\n            <v-radio label=\"Toggle this custom radio\" value=\"radio2\"></v-radio>\n            <v-radio label=\"Toggle this custom radio\" value=\"radio3\"></v-radio>\n          </v-radio-group>\n        </v-col>\n      </v-row>\n      <div class=\"mt-4\">\n        <v-select :items=\"items\" filled label=\"Input Select\" background-color=\"transparent\"></v-select>\n        <v-text-field\n          label=\"Helping text\"\n          filled\n          background-color=\"transparent\"\n          placeholder=\"Helping text\"\n          hint=\"A block of help text that breaks onto a new line and may extend beyond one line.\"\n          persistent-hint\n        ></v-text-field>\n      </div>\n      <v-btn class=\"text-capitalize mt-5 element-0\" color=\"success\">Submit</v-btn>\n    </v-card-text>\n  </v-card>\n</template>\n\n<script>\nexport default {\n  name: \"CardDefaultForm\",\n  data: () => ({\n    dtext: \"George deo\",\n    emailtext: \"\",\n    password: \"\",\n    placetext: \"\",\n    disableinput: \"\",\n    checkbox1: \"\",\n    checkbox2: \"\",\n    checkbox3: \"\",\n    show1: false,\n    radios: \"radio1\",\n    switch1: true,\n    switch2: false,\n    rules: {\n      required: value => !!value || \"Required.\",\n      min: v => v.length >= 8 || \"Min 8 characters\",\n      emailMatch: () => \"The email and password you entered don't match\"\n    },\n    items: [\"IT\", \"Blade Runner\", \"Thor\"]\n  })\n};\n</script>"]}]}