son']\"\n                chips\n                label=\"To\"\n                full-width\n                hide-details\n                hide-no-data\n                hide-selected\n                multiple\n                single-line\n                ></v-autocomplete>\n                <v-divider></v-divider>\n                <v-text-field\n                label=\"Subject\"\n                value=\"Plans for the weekend\"\n                single-line\n                full-width\n                hide-details\n                ></v-text-field>\n                <v-divider></v-divider>\n                <v-textarea\n                v-model=\"title\"\n                label=\"Message\"\n                counter\n                maxlength=\"120\"\n                full-width\n                single-line\n                ></v-textarea>\n            </v-form>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n  name: \"TextfieldFullwidthCharahterCounter\",\n\n  data: () => ({\n        selected: ['Trevor Handsen'],\n        items: ['Trevor Handsen', 'Alex Nelson'],\n        title: 'Hi,\\nI just wanted to check in and see if you had any plans the upcoming weekend. We are thinking of heading up to Napa',\n  })\n};\n</script>"]}]}