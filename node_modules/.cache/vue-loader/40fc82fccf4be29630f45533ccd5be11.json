n                v-model=\"model\"\n                class=\"pa-4\"\n                show-arrows\n                >\n                <v-slide-item\n                    v-for=\"n in 15\"\n                    :key=\"n\"\n                    v-slot:default=\"{ active, toggle }\"\n                >\n                    <v-card\n                    :color=\"active ? 'primary' : 'grey lighten-1'\"\n                    class=\"ma-4\"\n                    height=\"200\"\n                    width=\"100\"\n                    @click=\"toggle\"\n                    >\n                    <v-row\n                        class=\"fill-height\"\n                        align=\"center\"\n                        justify=\"center\"\n                    >\n                        <v-scale-transition>\n                        <v-icon\n                            v-if=\"active\"\n                            color=\"white\"\n                            size=\"48\"\n                            v-text=\"'mdi-close-circle-outline'\"\n                        ></v-icon>\n                        </v-scale-transition>\n                    </v-row>\n                    </v-card>\n                </v-slide-item>\n                </v-slide-group>\n\n                <v-expand-transition>\n                <v-sheet\n                    v-if=\"model != null\"\n                    color=\"grey lighten-4\"\n                    height=\"200\"\n                    tile\n                >\n                    <v-row\n                    class=\"fill-height\"\n                    align=\"center\"\n                    justify=\"center\"\n                    >\n                    <h3 class=\"title\">Selected {{ model }}</h3>\n                    </v-row>\n                </v-sheet>\n                </v-expand-transition>\n            </v-sheet>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n  name: \"GrpSlidePseudoCarousel\",\n\n  data: () => ({\n      model: null,\n  })\n};\n</script>"]}]}