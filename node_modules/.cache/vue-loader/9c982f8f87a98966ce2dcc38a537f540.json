/v-btn>\n                </template>\n\n                <v-list>\n                    <v-list-item\n                    v-for=\"(item, index) in items\"\n                    :key=\"index\"\n                    @click.stop\n                    >\n                    <v-list-item-title>{{ item.title }}</v-list-item-title>\n                    </v-list-item>\n                </v-list>\n                </v-menu>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n  name: \"MenusHover\",\n\n  data: () => ({\n      items: [\n        { title: 'Click Me' },\n        { title: 'Click Me' },\n        { title: 'Click Me' },\n        { title: 'Click Me 2' },\n      ],\n  })\n};\n</script>"]}]}