@click=\"chip1 = true, chip2 = true, chip3 = true, chip4= true\"\n                >\n                Reset Chips\n                </v-btn>\n\n                <v-chip\n                v-if=\"chip1\"\n                class=\"ma-2\"\n                close\n                @click:close=\"chip1 = false\"\n                >\n                Closable\n                </v-chip>\n\n                <v-chip\n                v-if=\"chip2\"\n                class=\"ma-2\"\n                close\n                color=\"error\"\n                text-color=\"white\"\n                @click:close=\"chip2 = false\"\n                >\n                Remove\n                </v-chip>\n\n                <v-chip\n                v-if=\"chip3\"\n                class=\"ma-2\"\n                close\n                color=\"success\"\n                outlined\n                @click:close=\"chip3 = false\"\n                >\n                Success\n                </v-chip>\n\n                <v-chip\n                v-if=\"chip4\"\n                class=\"ma-2\"\n                close\n                color=\"orange\"\n                label\n                outlined\n                @click:close=\"chip4 = false\"\n                >\n                Complete\n                </v-chip>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n  name: \"ChipsClosable\",\n\n  data: () => ({\n        chip1: true,\n        chip2: true,\n        chip3: true,\n        chip4: true,\n  })\n};\n</script>"]}]}