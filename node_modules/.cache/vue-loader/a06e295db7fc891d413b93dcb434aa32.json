l=\"message\"\n                        outlined\n                        clearable\n                        label=\"Message\"\n                        type=\"text\"\n                    >\n                        <template v-slot:prepend>\n                        <v-tooltip\n                            bottom\n                        >\n                            <template v-slot:activator=\"{ on }\">\n                            <v-icon v-on=\"on\">mdi-help-circle-outline</v-icon>\n                            </template>\n                            I'm a tooltip\n                        </v-tooltip>\n                        </template>\n                        <template v-slot:append>\n                        <v-fade-transition leave-absolute>\n                            <v-progress-circular\n                            v-if=\"loading\"\n                            size=\"24\"\n                            color=\"info\"\n                            indeterminate\n                            ></v-progress-circular>\n                            <img v-else width=\"24\" height=\"24\" src=\"https://cdn.vuetifyjs.com/images/logos/v-alt.svg\" alt=\"\">\n                        </v-fade-transition>\n                        </template>\n                        <template v-slot:append-outer>\n                        <v-menu\n                            style=\"top: -12px\"\n                            offset-y\n                        >\n                            <template v-slot:activator=\"{ on }\">\n                            <v-btn v-on=\"on\">\n                                <v-icon left>mdi-menu</v-icon>\n                                Menu\n                            </v-btn>\n                            </template>\n                            <v-card>\n                            <v-card-text class=\"pa-6\">\n                                <v-btn large flat color=\"primary\" @click=\"clickMe\"><v-icon left>mdi-target</v-icon>Click me</v-btn>\n                            </v-card-text>\n                            </v-card>\n                        </v-menu>\n                        </template>\n                    </v-text-field>\n                    </v-col>\n\n                </v-row>\n                </v-container>\n            </v-form>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n  name: \"TextfieldIconSlots\",\n\n  data: () => ({\n      message: 'Hey!',\n      loading: false,\n  }),\n  methods: {\n      clickMe () {\n        this.loading = true\n        this.message = 'Wait for it...'\n        setTimeout(() => {\n          this.loading = false\n          this.message = 'You\\'ve clicked me!'\n        }, 2000)\n      },\n    },\n};\n</script>"]}]}