y :value=\"overlay\">\n            <v-progress-circular indeterminate size=\"64\"></v-progress-circular>\n            </v-overlay>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n  name: \"OverlayLoader\",\n\n  data: () => ({\n      overlay: false,\n  }),\n    watch: {\n        overlay (val) {\n        val && setTimeout(() => {\n            this.overlay = false\n        }, 3000)\n        },\n    },\n};\n</script>"]}]}