teps\"\n                    :items=\"[2, 3, 4, 5, 6]\"\n                    label=\"# of steps\"\n                    ></v-select>\n                </v-card-text>\n                </v-card>\n                <v-stepper v-model=\"e1\">\n                <v-stepper-header>\n                    <template v-for=\"n in steps\">\n                    <v-stepper-step\n                        :key=\"`${n}-step`\"\n                        :complete=\"e1 > n\"\n                        :step=\"n\"\n                        editable\n                    >\n                        Step {{ n }}\n                    </v-stepper-step>\n\n                    <v-divider\n                        v-if=\"n !== steps\"\n                        :key=\"n\"\n                    ></v-divider>\n                    </template>\n                </v-stepper-header>\n\n                <v-stepper-items>\n                    <v-stepper-content\n                    v-for=\"n in steps\"\n                    :key=\"`${n}-content`\"\n                    :step=\"n\"\n                    >\n                    <v-card\n                        class=\"mb-12\"\n                        color=\"grey lighten-1\"\n                        height=\"200px\"\n                    ></v-card>\n\n                    <v-btn\n                        color=\"primary\"\n                        @click=\"nextStep(n)\"\n                    >\n                        Continue\n                    </v-btn>\n\n                    <v-btn text>Cancel</v-btn>\n                    </v-stepper-content>\n                </v-stepper-items>\n                </v-stepper>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n  name: \"SteppersDynamic\",\n\n  data: () => ({\n      e1: 1,\n        steps: 2,\n  }),\n  watch: {\n      steps (val) {\n        if (this.e1 > val) {\n          this.e1 = val\n        }\n      },\n    },\n\n    methods: {\n      nextStep (n) {\n        if (n === this.steps) {\n          this.e1 = 1\n        } else {\n          this.e1 = n + 1\n        }\n      },\n    },\n};\n</script>"]}]}