A,GAJA,EAIA;AACA,aAAA,MAAA,CAAA,MAAA,CAAA,uBAAA,EAAA,GAAA;AACA;AANA,KADA;AASA,IAAA,eAAA,EAAA;AACA,MAAA,GADA,iBACA;AACA,eAAA,KAAA,MAAA,CAAA,KAAA,CAAA,YAAA;AACA,OAHA;AAIA,MAAA,GAJA,eAIA,GAJA,EAIA;AACA,aAAA,MAAA,CAAA,MAAA,CAAA,mBAAA,EAAA,GAAA;AACA;AANA,KATA;AAiBA,IAAA,cAAA,EAAA;AACA,MAAA,GADA,iBACA;AACA,eAAA,KAAA,MAAA,CAAA,KAAA,CAAA,WAAA;AACA,OAHA;AAIA,MAAA,GAJA,eAIA,GAJA,EAIA;AACA,aAAA,MAAA,CAAA,MAAA,CAAA,kBAAA,EAAA,GAAA;AACA;AANA,KAjBA;AAyBA,IAAA,mBAAA,EAAA;AACA,MAAA,GADA,iBACA;AACA,eAAA,KAAA,MAAA,CAAA,KAAA,CAAA,mBAAA;AACA,OAHA;AAIA,MAAA,GAJA,eAIA,GAJA,EAIA;AACA,aAAA,MAAA,CAAA,MAAA,CAAA,YAAA,EAAA,GAAA;AACA;AANA;AAzBA,GAjCA;AAmEA,EAAA,KAAA,EAAA;AACA,IAAA,UADA,sBACA,GADA,EACA;AACA,WAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CACA,KAAA,MAAA,GAAA,MAAA,GAAA,OADA,EAEA,OAFA,GAEA,GAFA,EAGA,KAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CACA,KAAA,OAAA,GAAA,OAAA,GAAA,MADA,EAEA,OAFA,GAEA,GALA;AAMA,KARA;AASA,IAAA,eATA,2BASA,GATA,EASA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,mBAAA,EAAA,GAAA;AACA,KAXA;AAYA,IAAA,cAZA,0BAYA,GAZA,EAYA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,kBAAA,EAAA,GAAA;AACA,KAdA;AAeA,IAAA,mBAfA,+BAeA,GAfA,EAeA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,YAAA,EAAA,GAAA;AACA;AAjBA,GAnEA;AAsFA,EAAA,OAAA,EAAA;AAtFA,CAAA","sourcesContent":["<template>\n  <v-navigation-drawer\n    v-model=\"Customizer_drawer\"\n    app\n    temporary\n    :right=\"!$vuetify.rtl\"\n  >\n    <!---Layout Type -->\n    <div class=\"px-4 py-4\">\n      <span>Horizontal</span>\n      <v-switch\n        v-model=\"setHorizontalLayout\"\n        hide-details\n        class=\"mt-2\"\n        label=\"Horizontal\"\n      ></v-switch>\n    </div>\n    <v-divider></v-divider>\n\n    <div class=\"px-4 py-4\">\n      <span>RTL</span>\n      <v-switch\n        v-model=\"$vuetify.rtl\"\n        class=\"mt-2\"\n        hide-details\n        primary\n        label=\"Right to Left\"\n      />\n    </div>\n    <v-divider></v-divider>\n    <!---Sidebar Type -->\n    <div class=\"px-4 py-4\" v-if=\"!setHorizontalLayout\">\n      <span>Mini Sidebar</span>\n      <v-switch\n        v-model=\"internalValue\"\n        hide-details\n        class=\"mt-2\"\n        label=\"Mini variant\"\n      ></v-switch>\n    </div>\n    <v-divider></v-divider>\n    <!---Theme color -->\n    <div class=\"px-4 py-4\">\n      <span>Theme Color</span>\n      <v-item-group v-model=\"themecolor\">\n        <v-item\n          v-for=\"tcolor in themecolors\"\n          :key=\"tcolor\"\n          :value=\"tcolor\"\n          class=\"mt-2\"\n        >\n          <template v-slot=\"{ active, toggle }\">\n            <v-avatar\n              :class=\"active && 'v-sidebar_active'\"\n              :color=\"tcolor\"\n              class=\"v-settings__item mr-2\"\n              size=\"25\"\n              @click=\"toggle\"\n            />\n          </template>\n        </v-item>\n      </v-item-group>\n    </div>\n    <v-divider></v-divider>\n    <!---Navbar (Topbar) Color -->\n    <div class=\"px-4 py-4\">\n      <span>Topbar Colors</span>\n      <v-item-group v-model=\"setNavbarColor\">\n        <v-item\n          v-for=\"color in navbarColors\"\n          :key=\"color\"\n          :value=\"color\"\n          class=\"mt-2\"\n        >\n          <template v-slot=\"{ active, toggle }\">\n            <v-avatar\n              :class=\"active && 'v-sidebar_active'\"\n              :color=\"color\"\n              class=\"v-settings__item mr-2\"\n              size=\"25\"\n              @click=\"toggle\"\n            />\n          </template>\n        </v-item>\n      </v-item-group>\n    </div>\n    <!---End Sidebar Color -->\n    <v-divider></v-divider>\n    <!---Sidebar Color -->\n    <div class=\"px-4 py-4\">\n      <span>Sidebar Colors</span>\n      <v-item-group v-model=\"setSidebarColor\">\n        <v-item\n          v-for=\"color in SidebarColors\"\n          :key=\"color\"\n          :value=\"color\"\n          class=\"mt-2\"\n        >\n          <template v-slot=\"{ active, toggle }\">\n            <v-avatar\n              :class=\"active && 'v-sidebar_active'\"\n              :color=\"color\"\n              class=\"v-settings__item mr-2\"\n              size=\"25\"\n              @click=\"toggle\"\n            />\n          </template>\n        </v-item>\n      </v-item-group>\n    </div>\n    <!---End Sidebar Color -->\n  </v-navigation-drawer>\n</template>\n\n<script>\nimport Proxyable from \"vuetify/lib/mixins/proxyable\";\nexport default {\n  name: \"Customizer\",\n  mixins: [Proxyable],\n  data: () => ({\n    right: false,\n    SidebarColor: \"\",\n    SidebarColors: [\n      \"white\",\n      \"#1E1E1E\",\n      \"rgb(44, 59, 164)\",\n      \"rgb(96, 44, 164)\",\n      \"rgb(151, 210, 219)\",\n      \"rgb(77, 86, 100)\",\n    ],\n    navbarColor: \"#1e88e5\",\n    navbarColors: [\n      \"#1e88e6\",\n      \"#21c1d6\",\n      \"#fc4b6c\",\n      \"#563dea\",\n      \"white\",\n      \"#ff9800\",\n    ],\n    themecolor: \"#1e88e5\",\n    themecolors: [\n      \"#1e88e6\",\n      \"#21c1d6\",\n      \"#fc4b6c\",\n      \"#563dea\",\n      \"#46cc57\",\n      \"#ff9800\",\n    ],\n  }),\n  computed: {\n    Customizer_drawer: {\n      get() {\n        return this.$store.state.Customizer_drawer;\n      },\n      set(val) {\n        this.$store.commit(\"SET_CUSTOMIZER_DRAWER\", val);\n      },\n    },\n    setSidebarColor: {\n      get() {\n        return this.$store.state.SidebarColor;\n      },\n      set(val) {\n        this.$store.commit(\"SET_SIDEBAR_COLOR\", val);\n      },\n    },\n    setNavbarColor: {\n      get() {\n        return this.$store.state.navbarColor;\n      },\n      set(val) {\n        this.$store.commit(\"SET_NAVBAR_COLOR\", val);\n      },\n    },\n    setHorizontalLayout: {\n      get() {\n        return this.$store.state.setHorizontalLayout;\n      },\n      set(val) {\n        this.$store.commit(\"SET_LAYOUT\", val);\n      },\n    },\n  },\n  watch: {\n    themecolor(val) {\n      (this.$vuetify.theme.themes[\n        this.isDark ? \"dark\" : \"light\"\n      ].primary = val),\n        (this.$vuetify.theme.themes[\n          this.isLight ? \"light\" : \"dark\"\n        ].primary = val);\n    },\n    setSidebarColor(val) {\n      this.$store.commit(\"SET_SIDEBAR_COLOR\", val);\n    },\n    setNavbarColor(val) {\n      this.$store.commit(\"SET_NAVBAR_COLOR\", val);\n    },\n    setHorizontalLayout(val) {\n      this.$store.commit(\"SET_LAYOUT\", val);\n    },\n  },\n  methods: {},\n};\n</script>\n\n<style lang=\"scss\">\n.v-application .v-item-group .v-sidebar_active {\n  border: 1px solid black !important;\n}\n</style>"],"sourceRoot":"src/layouts/full-layout/customizer"}]}