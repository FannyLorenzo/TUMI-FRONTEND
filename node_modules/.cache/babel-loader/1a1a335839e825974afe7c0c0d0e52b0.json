A,EAAA,KAhCA;AAiCA,MAAA,SAAA,EAAA,EAjCA;AAkCA,MAAA,OAAA,EAAA;AAlCA,KAAA;AAAA,GAJA;AAyCA,EAAA,QAAA,EAAA,EAzCA;AA0CA,EAAA,OAAA,EAAA;AACA,IAAA,cADA,4BACA;AACA,WAAA,SAAA,GAAA,EAAA;;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,OAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,aAAA,SAAA,GAAA,KAAA,SAAA,GAAA,IAAA,GAAA,KAAA,OAAA,CAAA,CAAA,CAAA;AACA;AACA,KANA;AAOA,IAAA,QAPA,sBAOA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KATA;AAUA,IAAA,MAVA,oBAUA;AAAA;;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,QAAA;;AACA,UAAA,KAAA,KAAA,CAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,EAAA;AAGA,aAAA,OAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA;AACA,YAAA,IAAA,GAAA;AACA,sBAAA,KAAA,QADA;AAEA,sBAAA,KAAA;AAFA,SAAA;AAKA,QAAA,KAAA,CAAA,IAAA,CAAA,cAAA,EAAA,IAAA,EACA,IADA,CACA,UAAA,IAAA,EAAA;AACA,cAAA,IAAA,CAAA,IAAA,CAAA,KAAA,IAAA,IAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,IAAA,EADA,CAGA;;AACA,YAAA,YAAA,CAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,IAAA,CAAA,KAAA,EAJA,CAKA;;AACA,YAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,cAAA,IAAA,EAAA;AAAA,aAAA;AAEA,WARA,MAQA;AACA,YAAA,KAAA,CAAA,KAAA,GAAA,IAAA;AACA,YAAA,KAAA,CAAA,SAAA,GAAA,qDAAA;AACA;AAGA,SAhBA,EAiBA,KAjBA,CAiBA,UAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,KAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,qDAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,WAAA,EAAA,GAAA,CAAA,QAAA,CAAA,MAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EAAA,GAAA,CAAA,QAAA,CAAA,IAAA;;AACA,cAAA,GAAA,CAAA,QAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;;AACA,gBAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,gBAAA,IAAA,IAAA,EAAA;AACA,cAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,gBAAA,CAAA,CAAA,CAAA;AACA,aAFA,MAGA,IAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,QAAA,IAAA,IAAA,EAAA;AACA,cAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,QAAA,CAAA,CAAA,CAAA;AACA,aAFA,MAEA,IAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,QAAA,IAAA,IAAA,EAAA;AACA,cAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,QAAA,CAAA,CAAA,CAAA;AACA,aATA,CAUA;;;AACA,YAAA,KAAA,CAAA,cAAA;AACA;AACA,SAnCA;AAuCA;AAEA;AAhEA,GA1CA;AA4GA,EAAA,KAAA,EAAA;AACA,IAAA,MAAA,EAAA;AADA;AA5GA,CAAA","sourcesContent":["<template>\n  <v-container id=\"login\" class=\"fill-height justify-center\" tag=\"section\">\n    <v-row justify=\"center\">\n      <v-col lg=\"11\" sm=\"8\" xl=\"7\">\n        <v-card class=\"elevation-4\">\n          <v-row>\n            <v-col lg=\"7\" class=\"red lighten-1 d-none d-md-flex align-center justify-center\">\n              <div class=\"d-none d-sm-block\">\n                <div class=\"d-flex align-center pa-10\">\n                  <div>\n                    <h2\n                      class=\"display-1 white--text font-weight-bold\"\n                    >TUMI</h2>\n                    <h6\n                      class=\"subtitle-1 mt-4 white--text op-5 font-weight-bold\"\n                    >TU mercado, MI mercado</h6>\n                 </div>\n                </div>\n              </div>\n             \n                <v-carousel>\n                  <v-carousel-item\n                    v-for=\"(item,i) in items\"\n                    :key=\"i\"\n                    :src=\"item.src\"\n                    reverse-transition=\"fade-transition\"\n                    transition=\"fade-transition\"\n                  ></v-carousel-item>\n                </v-carousel>\n\n            </v-col>\n            <v-col lg=\"5\">\n              <div class=\"pa-7 pa-sm-12\">\n                <img src=\"@/assets/images/molinos/arroz.png\" />\n                <h2 class=\"font-weight-bold mt-4 white--text text--darken-2\">Iniciar sesión</h2>\n                <h6 class=\"subtitle-1\">\n                  Ingrese sus datos de usuario para iniciar sesión\n                </h6>\n\n                <v-form ref=\"form\" v-model=\"valid\" lazy-validation action=\"/tableros/indicadores\">\n                  <v-text-field\n                    v-model=\"username\"\n                    :rules=\"usernameRules\"\n                    label=\"Usuario\"\n                    class=\"mt-4\"\n                    required\n                    outlined\n                  ></v-text-field>\n                  <v-text-field\n                    v-model=\"password\"\n                    :counter=\"10\"\n                    :rules=\"passwordRules\"\n                    label=\"Contraseña\"\n                    required\n                    outlined\n                    :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\n                    :type=\"show1 ? 'text' : 'password'\"\n                  ></v-text-field>\n\n                  <div class=\"d-block d-sm-flex align-center mb-4 mb-sm-0\">\n                    <v-checkbox\n                      v-model=\"checkbox\"\n                      label=\"Recordarme?\"\n                      \n                    ></v-checkbox>\n                   \n                   <!--                    \n                    <div class=\"ml-auto\">\n                      <a href=\"javascript:void(0)\" class=\"link\">OLvidó su contraseña?</a>\n                    </div> \n                    -->\n                  </div>\n                   <v-alert \n                          dense\n                          outlined\n                          type=\"error\"\n                          v-if=\"error\"\n                        >\n                          {{msg_error}} <strong>!</strong> </v-alert>\n                  <v-btn\n                    :disabled=\"!valid\"\n                    color=\"#EF5350\"\n                    block\n                    class=\"mr-4\"\n                    submit\n                    @click=\"submit\"\n                  >Iniciar sesión</v-btn>  \n                  <div class=\"d-block d-sm-flex align-center mb-4 mb-sm-0\">                                     \n                    <div class=\"ml-auto\">\n                      <a href=\"javascript:void(0)\" class=\"link\">_</a>\n                    </div>                  \n                  </div>\n                    <v-btn\n                    color=\"#EF5350\"                   \n                    class=\"mr-4\"\n                    block\n                    submit\n                    @click=\"register\"\n                  >Registrarse</v-btn>                \n                  \n                </v-form>          \n         \n              </div>\n            </v-col>         \n          </v-row>\n        </v-card>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\n\nimport axios from 'axios';\nexport default {\n  \n  name: \"login\",\n\n  data: () => ({\n    // carrusel \nitems: [\n          {\n            src: 'https://cdn.vuetifyjs.com/images/carousel/squirrel.jpg',\n          },\n          {\n            src: 'https://cdn.vuetifyjs.com/images/carousel/sky.jpg',\n          },\n          {\n            src: 'https://cdn.vuetifyjs.com/images/carousel/bird.jpg',\n          },\n          {\n            src: 'https://cdn.vuetifyjs.com/images/carousel/planet.jpg',\n          },\n        ],\n    // fin carrusel\n    valid: true,\n    password: \"\",\n    show1: false,\n    passwordRules: [\n      v => !!v || \"Contraseña es requerida\",\n      v => (v && v.length <= 10) || \"La contraseña debe ser menor a 10 carateres\"\n    ],\n    username: \"\",\n    usernameRules: [\n      v => !!v || \"El usuario es requerido\"//,\n      // => (v && v.length <= 10) || \"El nombre debe tener menos de 10 caracteres\"\n    ],\n    \n    checkbox: false,\n    \n    error : false,\n    msg_error : \"\",\n    errores : [],\n\n  }),\n  computed: {},\n  methods: {\n    mostrarErrores(){\n      this.msg_error = \"\";\n      for(let i = 0; i<this.errores.length; i++){\n          this.msg_error = this.msg_error +\"\\n\"+this.errores[i];  \n      }\n    },\n    register(){\n       this.$router.push({ path: \"/registro\" });\n    },\n    submit() {\n      this.$refs.form.validate();\n      if (this.$refs.form.validate(true)) {\n          \n\n      this.errores = [];\n      console.log(this.username);\n      console.log(this.password);\n      let json = {\n        \"username\" : this.username,\n        \"password\": this.password,\n\n      };\n      axios.post('/auth/login/', json)\n      .then(data =>{\n        if(data.data.token != null){\n        console.log(data);\n\n        //localStorage.token = data.data.token;\n        localStorage.setItem(\"token\", data.data.token);\n       // console.log(localStorage.getItem(\"token\"));\n        this.$router.push({ path: \"/tableros/indicadores\" });\n\n        }else{\n          this.error = true;\n          this.msg_error= \"Revise los datos ingresado, no puede iniciar sesión\" \n        }\n        \n\n      })\n      .catch(err => {\n        this.error = true;\n          this.msg_error= \"Revise los datos ingresado, no puede iniciar sesión\"\n        console.log(\" status: \", err.response.status);\n        console.log(\" data: \", err.response.data);\n        if(err.response.status==400){\n          console.log(err.response.data);\n          if(err.response.data.non_field_errors!=null){\n            this.errores.push(err.response.data.non_field_errors[0]);\n          }\n          else if(err.response.data.username!=null){\n            this.errores.push(err.response.data.username[0]);\n          }else if(err.response.data.password!=null){\n            this.errores.push(err.response.data.password[0]);\n          }\n          //this.msg_error=err.response.data.non_field_errors[0];\n        this.mostrarErrores();\n        }\n      })\n\n      \n        \n      }\n\n    }\n  },\n    props: {\n    source: String\n  }\n};\n</script>\n"],"sourceRoot":"src/views/usuarios"}]}