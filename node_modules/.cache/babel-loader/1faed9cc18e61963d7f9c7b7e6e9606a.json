SALA;AAKA,qBAAA,GALA;AAMA,oBAAA,WANA;AAMA,oBAAA,UANA;AAOA,6BAAA,CAAA,kBAPA;AAQA,8BAAA,CAAA,iBARA;AASA,sBAAA,CATA;AASA,mBAAA;AATA,OADA,EAYA;AACA,mBAAA,UADA;AAEA,oBAAA,SAFA;AAGA,wBAAA,GAHA;AAIA,kBAAA,UAJA;AAIA,6BAAA,EAJA;AAKA,kBAAA,YALA;AAMA,qBAAA,CANA;AAOA,oBAAA,aAPA;AAQA,oBAAA,WARA;AASA,6BAAA,GATA;AAUA,8BAAA,GAVA;AAWA,sBAAA,CAXA;AAYA,mBAAA;AAZA,OAZA;AAZA,KAAA;AAAA,GAFA;AA2CA,EAAA,OA3CA,qBA2CA;AACA,SAAA,MAAA;AACA,GA7CA;AA8CA,EAAA,OAAA,EAAA;AACA,IAAA,KADA,iBACA,GADA,EACA;AAEA,WAAA,OAAA,CAAA,IAAA,CACA;AACA,QAAA,IAAA,EAAA,aADA;AAEA,QAAA,MAAA,EAAA;AACA,UAAA,EAAA,EAAA;AADA;AAFA,OADA;AAQA,KAXA;AAaA,IAAA,MAbA,oBAaA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,mDAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,cAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,EAAA,CAAA,aAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,YAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,EAAA,CAAA,OAAA;AAEA,OANA,EAOA,KAPA,CAOA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,OATA;AAUA;AAzBA,GA9CA;AAyEA,EAAA,QAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,aAAA;AACA,KAHA;AAKA,IAAA,YALA,0BAKA;AAAA;;AACA,aAAA,KAAA,aAAA,CAAA,MAAA,CACA,UAAA,WAAA;AAAA,eACA,WAAA,CAAA,MAAA,CACA,WADA,GAEA,QAFA,CAEA,KAAA,CAAA,MAAA,CAAA,WAAA,EAFA,KAGA,WAAA,CAAA,QAAA,CAAA,WAAA,GAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAAA,WAAA,EAAA,CAJA;AAAA,OADA,CAAA;AAOA;AAbA;AAzEA,CAAA","sourcesContent":["<template>\n  <v-container fluid class=\"down-top-padding\">\n    <v-card class=\"mb-7\">\n      <v-card-text class=\"pa-5\">\n        <v-row justify=\"space-between\">\n          <v-col cols=\"12\" lg=\"4\">\n            <v-text-field\n              v-model=\"search\"\n              prepend-inner-icon=\"mdi-magnify\"\n              label=\"Search Contacts\"\n              filled\n              background-color=\"transparent\"\n              hide-details\n            ></v-text-field>\n          </v-col>\n          \n        </v-row>\n      </v-card-text>\n    </v-card>\n\n    <v-row justify=\"center\">\n      <v-col cols=\"12\" sm=\"6\" lg=\"4\" v-for=\"(contactgrid, index) in contactgrids\" :key=\"index\">\n        <v-card class=\"mb-7\">\n          <v-card-text class=\"pa-5 text-center\">\n            <img\n              :src=\"contactgrid.foto\"\n              alt=\"user\"\n              class=\"rounded-circle\"\n              width=\"200\"\n            />\n            <h4 class=\"title mt-3 mb-0\">{{ contactgrid.nombre }} {{ contactgrid.apellido }}</h4>\n            <div class=\"mt-3\">\n              <v-btn depressed small color=\"orange\" class=\"mr-2\" @click=\"toDir(contactgrid.usuario_id)\" >Perfil</v-btn>\n          \n            </div>\n            <div class=\"mt-3 pt-3\">\n              <address class=\"subtitle-2 font-weight-regular\">{{ contactgrid.correo }}</address>\n            </div>\n          </v-card-text>\n\n          <div class=\"pa-3 blue text-center\">\n            <h4 style=\"text-align: center;\">Calificaci√≥n</h4> \n             <v-rating                \n                :value=\"contactgrid.calificacion / contactgrid.npersonas\"\n                background-color=\"yellow\"\n                color=\"yellow\"\n                large\n                ></v-rating>\n               \n          </div>\n          <v-card-text class=\"px-4 py-3\">\n            <v-row>\n            \n                <v-chip pill>\n                  <v-avatar left>\n                    <v-btn color=\"light-green accent-3\" class=\"white--text\">\n                      <v-icon small>mdi-whatsapp</v-icon>\n                    </v-btn>\n                  </v-avatar>\n                  {{ contactgrid.telefono }}\n                  \n                </v-chip>            \n                <v-chip pill>\n                  <v-avatar left>\n                    <v-btn color=\"pink darken-1\" class=\"white--text\">\n                      <v-icon small>mdi-web</v-icon>\n                    </v-btn>\n                  </v-avatar>\n                  {{ contactgrid.website }}\n                </v-chip>\n             \n              \n            </v-row>\n          </v-card-text>\n        </v-card>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n\n<script>\n\nimport axios from \"axios\";\n\nexport default {\n  name: \"productores\",\n  data: () => ({\n    valid: true,\n    dialog: false,\n    search: \"\",\n    contactname: \"\",\n    phone: \"\",\n    role: \"\",\n    img: \"/1.jpg\",\n    address: \"\",\n    twitterfollowers: \"\",\n    instafollowers: \"\",\n    fbfollowers: \"\",\n    contactfilter: [\n{\"DNI_RUC\":\"98765432\",\n\"apellido\":\"apellido2\",\n\"calificacion\":202.0,\n\"correo\":\"correo2@gmail.com\",\n\"descripcion_larga\":\"descripcion2\",\n\"nombre\":\"nombre2\",\"npersonas\":120,\n\"password\":\"password2\",\"telefono\":\"12345678\",\n\"ubicacion_map_lat\":-16.398612833254646,\n\"ubicacion_map_long\":-71.53695924733312,\n\"usuario_id\":3,\"website\":\"lalalpansitos2.com\"\n},\n{\n  \"DNI_RUC\":\"78007943\",\n\"apellido\":\"Lorenzo\",\n\"calificacion\":0.0,\n\"correo\":\"{correo}\",\"descripcion_larga\":\"\",\n\"nombre\":\"Nombrecito\",\n\"npersonas\":0,\n\"password\":\"contrasena1\",\n\"telefono\":\"946748360\",\n\"ubicacion_map_lat\":0.0,\n\"ubicacion_map_long\":0.0,\n\"usuario_id\":5,\n\"website\":\"\"},\n\n    ],\n   \n  }),\n  created(){\n    this.listar();\n  },\n  methods: {\n     toDir(cod){      \n       \n      this.$router.push(\n        {\n          name: \"Productores\",\n          params:{\n            id: cod,\n          }\n        }     \n      );\n    },\n  \n     listar(){\n      let my = this;                  \n        axios.get(\"https://capacitacion-docente.herokuapp.com/users/\").then(function(response){\n       console.log(\"datadata \"+response.data.data);\n       my.contactfilter = response.data.data;\n       console.log(\"entro okii\");\n          console.log(my.options);\n          \n          })\n        .catch(err => {\n          console.log(err.response.data)\n        });\n    },\n  },\n  computed: {\n    events() {\n      return this.$store.state.contactEvents;\n    },\n   \n    contactgrids() {\n      return this.contactfilter.filter(\n        contactgrid =>\n          contactgrid.nombre\n            .toLowerCase()\n            .includes(this.search.toLowerCase()) ||\n          contactgrid.apellido.toLowerCase().includes(this.search.toLowerCase())\n      );\n    },\n    \n  }\n};\n</script>"],"sourceRoot":"src/views/productores"}]}