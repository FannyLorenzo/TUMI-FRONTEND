KAZA;AAaA,IAAA,aAAA,EAAA,yBAAA;AAAA;;AACA,aAAA,IAAA,OAAA,CAAA,UAAA,OAAA,EAAA,MAAA,EAAA;AACA,QAAA,UAAA,CAAA,YAAA;AACA,cAAA,KAAA,CAAA,KAAA,GAAA,CAAA,EAAA;AACA,YAAA,KAAA,CAAA,KAAA;AACA,YAAA,MAAA,CACA,0FADA,CAAA;AAGA,WALA,MAKA;AACA,YAAA,KAAA,CAAA,KAAA,GAAA,CAAA;AACA,YAAA,OAAA,CAAA,IAAA,CAAA;AACA;AACA,SAVA,EAUA,IAVA,CAAA;AAWA,OAZA,CAAA;AAaA;AA3BA;AAZA,CAAA","sourcesContent":["<template>\n  <div>\n    <form-wizard\n      @on-complete=\"onComplete\"\n      @on-loading=\"setLoading\"\n      @on-validate=\"handleValidation\"\n      @on-error=\"handleErrorMessage\"\n      shape=\"circle\"\n      color=\"#1e88e5\"\n      error-color=\"#e74c3c\"\n    >\n      <tab-content title=\"Personal details\" :before-change=\"validateAsync\" icon=\"ti-user\">First tab</tab-content>\n      <tab-content title=\"Additional Info\" icon=\"ti-settings\">Second tab</tab-content>\n      <tab-content title=\"Last step\" icon=\"ti-check\">Third tab</tab-content>\n\n      <div class=\"loader\" v-if=\"loadingWizard\"></div>\n      <div v-if=\"errorMsg\">\n        <span class=\"error error--text\">{{errorMsg}}</span>\n      </div>\n    </form-wizard>\n  </div>\n</template>\n\n<script>\nimport { FormWizard, TabContent } from \"vue-form-wizard\";\nimport \"vue-form-wizard/dist/vue-form-wizard.min.css\";\n\nexport default {\n  name: \"FormWizardValidation\",\n  data: () => ({\n    title: \"FormWizardValidation\",\n    loadingWizard: false,\n    errorMsg: null,\n    count: 0\n  }),\n  components: {\n    FormWizard,\n    TabContent\n  },\n  methods: {\n    onComplete: function() {\n      alert(\"Yay. Done!\");\n    },\n    setLoading: function(value) {\n      this.loadingWizard = value;\n    },\n    handleValidation: function(isValid, tabIndex) {\n      console.log(\"Tab: \" + tabIndex + \" valid: \" + isValid);\n    },\n    handleErrorMessage: function(errorMsg) {\n      this.errorMsg = errorMsg;\n    },\n    validateAsync: function() {\n      return new Promise((resolve, reject) => {\n        setTimeout(() => {\n          if (this.count < 1) {\n            this.count++;\n            reject(\n              \"This is a custom validation error message. Click next again to get rid of the validation\"\n            );\n          } else {\n            this.count = 0;\n            resolve(true);\n          }\n        }, 1000);\n      });\n    }\n  }\n};\n</script>"],"sourceRoot":"src/views/form-layouts/form-wizard/form-wizard-validation"}]}